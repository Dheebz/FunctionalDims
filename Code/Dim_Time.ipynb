   "source": [
    "from datetime import datetime , timedelta\n",
    "from tqdm import tqdm\n",
    "import pandas as pd \n",
    "import time"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "Generating Dim Time data betweem 1900-01-01 00:00:00 and 1900-01-02 00:00:00 with interval 1:00:00:   0%|          | 0/24 [00:00<?, ?it/s]"
     ]
    }
   ],
   "source": [
    "\n",
    "\n",
    "#Modify desired interval (seconds,minutes,hours)\n",
    "interval = timedelta(hours=1)\n",
    "#TimeStart and TimeFinal do not need to be modified unless you want intervals less than 24 hours
    "TimeStart = datetime(1900, 1, 1, 0, 0, 0)\n",
    "TimeFinal = datetime(1900, 1, 2, 0, 0, 0)\n",
    "\n",
    "Rng = tqdm(range(int((TimeFinal - TimeStart).total_seconds() / interval.total_seconds())),desc=f'Generating Dim Time data betweem {TimeStart} and {TimeFinal} with interval {interval}')\n",
    "records = []"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "Generating Dim Time data betweem 1900-01-01 00:00:00 and 1900-01-02 00:00:00 with interval 1:00:00: 100%|██████████| 24/24 [00:12<00:00,  1.99it/s]\n"
     ]
    }
   ],
   "source": [
    "for iteration in Rng:\n",
    "    record = {\n",
    "        #TimeVal = time 'HH:mm:ss'\n",
    "        'TimeVal': (TimeStart + (iteration * interval)).strftime('%H:%M:%S'),\n",
    "        #TimeKey = time 'HHmmss'\n",
    "        'TimeKey': int((TimeStart + (iteration * interval)).strftime('%H%M%S')),\n",
    "        #HourInt\n",
    "        'HourInt': int((TimeStart + (iteration * interval)).strftime('%H')),\n",
    "        #MinuteInt\n",
    "        'MinuteInt': int((TimeStart + (iteration * interval)).strftime('%M')),\n",
    "        #SecondInt\n",
    "        'SecondInt': int((TimeStart + (iteration * interval)).strftime('%S')),\n",
    "    }\n",
    "    records.append(record)\n",
    "    time.sleep(.5)"
   ]
  },
